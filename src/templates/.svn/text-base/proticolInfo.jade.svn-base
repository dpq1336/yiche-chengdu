extends templateProticol
block titleAndStyles
    title 项目合同详情页
    style.
        body{
            word-break:break-all;
            word-wrap:break-word;
        }
block scripts
    script.
        //-
        seajs.use(["zepto", "util"], function (a, util) {
            var pageController = {
                init: function () {
                    util.getWeiboApi();
                    this.getPageData();
                },
                store:{
                    serverIP: util.getServerIp(),
                    userId: util.getUserId(),
                    loanId: util.getUrlParam("loanId"),
                    id: util.getUrlParam("id"),
                    loanInvestorId: util.getUrlParam("loanInvestorId"),
                    contractType: util.getUrlParam("contractType"),
                    baseCode:util.getUrlParam("baseCode")

                },
                getPageData: function () {
                    var _this = this;
                    var urljson = '/app/loan/findLoanContractDescByIdOrLoanId' ;
                    var datajson = {
                        userId: _this.store.userId,
                        loanId: _this.store.loanId,
                        id: _this.store.id
                    };
                    if( _this.store.loanInvestorId ){
                        urljson = '/app/h5/usercenter/getDynamicContract' ;
                        datajson = {
                            userId: _this.store.userId,
                            loanInvestorId: _this.store.loanInvestorId,
                            contractType: _this.store.contractType,
                            loanId: _this.store.loanId,
                            baseCode:_this.store.baseCode
                        };
                    }
                    $.ajax({
                        type: "POST",
                        url: _this.store.serverIP + urljson,
                        dataType: "json",
                        data: datajson,
                        success: function (jsonData) {
                            if (jsonData && jsonData.control.error == 0) {
                                var data = jsonData.data;
                                $('.page').html( data.content );
                                $("table").css("width","100%");
                            }
                        },
                        error: function (jsonData) {
                            util.errorHandler(_this.store.userId);
                        }
                    });
                }
            }
            $(function () {
                pageController.init();
            });
        });

block content
    //-
    div.page-group
        div.page.page-current(style="margin:0 auto; padding:20px 10px 50px")
            div.loading_img_x
                img(src="data:image/gif;base64,")






















